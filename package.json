{
  "name": "medical-centre-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:fake:test": "cd server && npx ts-node fake/test.ts",
    "generate:fake:database": "cd server && npx ts-node scripts/generate-db.ts",
    "generate:types": "openapi-typescript contract/openapi.yml --output contract/contract.ts",
    "watch:contract": "nodemon --watch contract/openapi.yml --ext yaml,yml --exec 'npm run generate:types'",
    "contract:validate": "redocly lint contract/openapi.yml",
    "contract:generate-docs": "redocly build-docs contract/openapi.yml && mkdir -p docs && mv redoc-static.html docs/index.html && touch docs/.nojekyll",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build -o dist-storybook",
    "dev:client": "concurrently \"npm run watch:contract\" \"npm run vite\" \"npm run storybook\"",
    "dev:server": "cd server && npm run dev"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@react-pdf/renderer": "^4.1.5",
    "@redocly/cli": "^1.27.1",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.344.0",
    "msw": "^2.6.8",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "remark-gfm": "^3.0.1",
    "swagger-ui-react": "^5.18.2",
    "tailwind-merge": "^2.2.1",
    "vite-plugin-svgr": "^4.3.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/js": "^9.9.1",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/swagger-ui-react": "^4.18.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "openapi-typescript": "^7.4.4",
    "postcss": "^8.4.35",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
